{
  "name": "test",
  "version": "1.0.0",
  "description": "lodash and config tests",
  "main": "src/index.js",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "jest-cli": "^26.2.2",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "prettier": "prettier \"src/**/*.js\" --write",
    "watch-prettier": "onchange \"src/**/*.js\" -- npm run prettier",
    "watch-run": "onchange \"src/**/*.js\" -- npm start"
  },
  "jest": {
    "testMatch": [
      "**/*.spec.js"
    ],
    "testURL": "http://localhost/"
  },
  "author": "classProgrammer",
  "license": "MIT"
}
